<div class="blue-banner">
	<div class="container">
		<div class="col-lg-12">
			<h3>{{property.display_address}}, {{property.price}}</h3>
			<h4>{{property.bedrooms}} bedrooms</h4>
		</div>
	</div>
</div>
<div class="container">
	<div class="row">
		<div class="col-lg-7">
			<div class="photo-slider">
				<img src="{{property.photos.first | url_for_property_asset : '638x425'}}" alt="">
				<div class="photo-reel">
					{% for photo in property.photos %}
						<img src="{{photo | url_for_property_asset : "110x74" }}">
                    {% endfor %}
				</div>
			</div>
		</div>
		<div class="col-lg-5">
			<h4>Call us on 01392 182 833 or fill out the form below.</h4>
			<form action="/properties/{{property.property_id}}/{{property.primary_channel}}/leads" id="contact_form" class="generic clearfix" method="post">
				<h2 id="make_enquiry">Contact our {{ branch.name }} office about this property</h2> 
					<label><span>First Name</span><input name="lead_client[first_name]" type="text"></label>
					<label><span>Last Name</span><input name="lead_client[last_name]" type="text"></label>
					<label><span>Email</span><input name="lead_client[email]" type="text"></label>
					<label><span>Telephone</span><input name="lead_client[tel_home]" type="text"></label>
					<label><span>Message</span><textarea name="lead[message]"></textarea></label>
			</form>
		</div>
	</div>
</div>
<div class="container">
	<div class="col-lg-7">
		{{property.description}}
	</div>
	<div class="col-lg-5">
	
	</div>
</div>