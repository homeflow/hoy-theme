<div class="blue-banner">
	<div class="container">
		<div class="col-lg-12">
			<h3>{{property.display_address}}, {{property.price}}</h3>
			<h4>{{property.bedrooms}} bedrooms</h4>
		</div>
	</div>
</div>
<div class="container">
	<div class="row">
		<div class="col-lg-7">
			<div class="photo-slider">
				<img src="{{property.photos.first | url_for_property_asset : '638x425'}}" alt="">
				<div class="photo-reel">
					{% for photo in property.photos %}
						<img src="{{photo | url_for_property_asset : "110x74" }}">
                    {% endfor %}
				</div>
			</div>
		</div>
		<div class="col-lg-5">
			<h4>Call us about this property:<h4>
			<h4>01392 182 833</h4>
			<h4>
				<a href="#">Request Details</a>
			</h4>
		</div>
	</div>
</div>
<div class="container">
	<div class="col-lg-7">
		{{property.description}}
	</div>
	<div class="col-lg-5">
	
	</div>
</div>